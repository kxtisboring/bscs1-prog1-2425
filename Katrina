#include <iostream>

using namespace std;

int main() {
  
  int intNum = 10;
  float floatNum = 7.5;
  double doubleNum = 7.5;
  char charLetter = 'K';
  bool boolValue = true;
  string stringText = "Hello,Katrina! 9876543210";
  string firstName = "Katrina";
  string middleName = "Saliente";
  string lastName = "Aniceto";
  
  string fullName;
  
  fullName = firstName +""+ middleName +""+ lastName;
  
  cout << "Integer:"<< intNum << endl;
  cout << "Float:"<< floatNum << endl;
  cout << "Double:"<< doubleNum << endl;
  cout << "Character:"<< charLetter << endl;
  cout << "Boolean:"<< boolValue << endl;
  cout << "String:"<< stringText << endl;
  cout << fullName << endl;
  
  return 0;
}
